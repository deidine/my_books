.announcement, .sale { display: none; height: auto; padding: 10px; position: relative; opacity: 0; text-align: center; z-index: 99; width: 100%; }

.announcement .close, .sale .close { color: rgb(255, 255, 255); font-weight: 700; margin-top: -10px; position: absolute; right: 10px; top: 50%; }

.announcement { color: rgb(255, 255, 255); background: rgb(243, 244, 246); z-index: 99999; }

.announcement.active { display: block; opacity: 1; }

.announcement .btn { border: 1px solid rgb(0, 148, 201); border-radius: 3px; font: 500 13px Roboto, "helvetica neue", Helvetica, Arial, sans-serif; margin: 10px 5px 0px; padding: 6px 10px; color: rgb(255, 255, 255); background-color: rgb(0, 148, 201); }

.announcement .btn.btn-green { border: 1px solid rgb(131, 193, 31); color: rgb(255, 255, 255); background: rgb(131, 193, 31); text-shadow: transparent 0px 0px; font-weight: 700; }

.announcement .btn.btn-green:hover { color: rgb(131, 193, 31); }

.announcement .btn:hover { background: rgb(255, 255, 255); border-color: rgb(255, 255, 255); color: rgb(0, 148, 201); }

.announcement .wrapper { padding: 0px; }

.announcement .wrapper a.btn { margin: 0px 0px 0px 15px; color: rgb(28, 35, 46) !important; }

@media (max-width: 1018px) {
  .announcement .wrapper a.btn { max-width: none !important; display: inline-block !important; }
}

.announcement p { margin: 0px; font-weight: 700; }

.announcement .close { background: rgba(131, 193, 31, 0.7); border-radius: 100%; }

@media (max-width: 1018px) {
  .announcement { min-height: 109px; }
}

.sale { background: rgb(46, 57, 66); transition: all 0.5s ease-in 0s; }

.sale.active { display: block; padding: 0px 0px 10px; opacity: 1; z-index: 10000; }

.sale.active.sale-banner-shop { position: absolute !important; }

.sale.active .flag { display: block; margin-bottom: 10px; }

.sale.active .flag img { display: inline-block; margin-top: 4px; line-height: 57px; }

.sale .btn { border: 1px solid rgb(0, 148, 201); border-radius: 3px; font: 500 13px Roboto, "helvetica neue", Helvetica, Arial, sans-serif; margin: 10px 5px 0px; padding: 6px 10px; color: rgb(255, 255, 255); background-color: rgb(0, 148, 201); }

.sale .btn:hover { background: rgb(255, 255, 255); border-color: rgb(255, 255, 255); color: rgb(0, 148, 201) !important; }

.sale .sale-crown { color: rgb(255, 210, 0); display: block; margin-right: 10px; font-size: 1.275em; float: left; margin-top: -4px; }

.sale h3, .sale p { color: rgb(255, 255, 255); }

.sale h3 { margin: 0px 10px 0px 0px; vertical-align: middle; text-transform: uppercase; font: 700 1em Roboto, "helvetica neue", Helvetica, Arial, sans-serif !important; }

.sale h3 .icon-crown { color: rgb(255, 210, 0); display: inline-block; vertical-align: text-top; margin-right: 10px; }

.sale h3 p { display: inline; font-weight: 700; }

.sale .sale-desc { display: block; font-size: 14px; margin: 5px 10px 4px 0px; text-align: center; vertical-align: middle; line-height: 18px; }

.sale .sale-desc .sub_desc { display: block; font-size: 12px; margin-top: 4px; }

.sale .header-search-form .btn:hover span { color: rgb(0, 148, 201); }

@media only screen and (min-width: 1020px) {
  .sale .btn, .sale .sale-desc, .sale h3 { display: inline-block; }
  .sale .wrapper { padding-top: 0px; max-width: 840px; }
  .sale.active { padding: 10px 0px; z-index: 10000; }
  .sale.active strong { font-weight: 800; }
  .sale.active .flag { width: 20px; float: left; margin-top: -3px; margin-right: 12px; margin-bottom: 0px; }
  .sale.active .flag img { box-shadow: rgba(50, 50, 50, 0.2) 0px 2px 3px 0px; }
  .sale h3 { font-size: 1em !important; }
  .sale .sale-desc { font-size: 14px; margin: 0px; }
  .sale .sale-desc small { font-size: 0.845em; }
  .sale .btn { margin: 0px 0px 0px 10px; vertical-align: middle; }
  .sale h3 { margin: 0px 20px 0px 0px; }
}

.drop-shadow { box-shadow: rgba(50, 50, 50, 0.81) 0px 3px 8px 0px; }

.announcement.active, .sale.sale-no-user.active { position: fixed; left: 0px; top: 0px; }

.announcement.active + header, .sale.sale-no-user.active + header { padding-top: 60px; }

@media (max-width: 1019px) {
  .announcement.active + header, .sale.sale-no-user.active + header { padding-top: 123px; }
}

@media (max-width: 498px) {
  .announcement.active + header, .sale.sale-no-user.active + header { padding-top: 109px; }
}

